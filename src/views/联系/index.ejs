<body class="page-about" id="page-index">

  <main class="comp-root">
    <!-- header -->
    <section class="comp-root-header">
      <%= require('../../components/head/index.ejs')() %>
    </section>
    <!-- menu -->
    <section class="comp-root-menu">
      <%= require('../../components/menu/index.ejs')() %>
    </section>
    <!-- banner -->
    <section class="about-banner">
      <div class="about-banner-img" style="background-image: url(<%= require('./images/contact.jpg') %>)">
      </div>
      <div class="about-box">
        <div class="about-box-home">
          <%= require('../../components/home/index.ejs')() %>
        </div>
      </div>
    </section>

    <!-- body -->
    <section class="content-body ">
      <div class="content-body-text col-md-12"> <%= require('../../components/title/index.ejs')() %></div>
      <div class="row content-body-content">
        <div class="col-md-12 content-body-content-title">Iris Brilliant Company Limited. </div>
        <div class="col-md-12 row">
          <div class="col-md-6">
            <h1>Address</h1>
            <p>NO. 7 YINCHUANXI ROAD, QINGDAO 266071, CHINA</p>
          </div>
          <div class="col-md-6">
            <h1>Address</h1>
            <p>NO. 7 YINCHUANXI ROAD, QINGDAO 266071, CHINA</p>
          </div>
        </div>
        <div class="col-md-12 row">
          <div class="col-md-6">
            <h1>Address</h1>
            <p>NO. 7 YINCHUANXI ROAD, QINGDAO 266071, CHINA</p>
          </div>
          <div class="col-md-6">
            <h1>Address</h1>
            <p>NO. 7 YINCHUANXI ROAD, QINGDAO 266071, CHINA</p>
          </div>
        </div>
      </div>
    </section>

    <section class="from">
      <div class="from-box">
        <div class="from-box-text">Feedback with this form</div>
        <div class="from-box-input">
          <input type="text" required>
          <p>Name</p>
        </div>
        <div class="from-box-input">
          <input type="text" required>
          <p>E-mail</p>
        </div>
        <div class="from-box-textarea">
          <p class="from-box-input">Message</p>
          <textarea type="text"></textarea>

        </div>
        <div class="from-box-input">
          <input type="text" required>
          <p>Code</p>
          <img class="vd" src="<%= require('./images/vd.jpg') %>" alt="">
        </div>
        <div class="submit">
          <div><a href="">SUBMIT</a></div>
        </div>
      </div>

    </section>
    <!-- footer -->
    <section>
      <%= require('../../components/footer/index.ejs')() %>
    </section>

  </main>
  <%= require('../../components/webmap/index.ejs')() %>
</body>

</html>
